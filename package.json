{
  "name": "react-native-expo-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:client": "expo start --dev-client",
    "start:clean": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "ios:prebuild": "expo prebuild --platform ios",
    "ios:prebuild:clean": "expo prebuild --platform ios --clean",
    "ios:fresh": "expo prebuild --platform ios --clean && expo run:ios",
    "ios:build:dev": "eas build --profile development --platform ios",
    "ios:build:sim": "eas build --profile development-simulator --platform ios",
    "ios:build:preview": "NODE_ENV=development eas build --profile preview --platform ios",
    "ios:build:prod": "NODE_ENV=production eas build --profile production --platform ios",
    "ios:submit": "eas submit -p ios --latest",
    "android:prebuild": "expo prebuild --platform android",
    "android:prebuild:clean": "expo prebuild --platform android --clean",
    "android:fresh": "expo prebuild --platform android --clean && expo run:android",
    "android:build:dev": "eas build --profile development --platform android",
    "android:build:preview": "NODE_ENV=development eas build --profile preview --platform android",
    "android:build:prod": "NODE_ENV=production eas build --profile production --platform android",
    "android:submit": "eas submit -p android --latest",
    "cleanup": "node scripts/cleanup.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@niondigital/widgets-expo-config-plugin": "^1.1.1",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/elements": "^2.8.2",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@react-navigation/stack": "^7.6.2",
    "@shopify/flash-list": "2.0.2",
    "@shopify/react-native-skia": "2.2.12",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.1",
    "expo": "~54.0.32",
    "expo-application": "~7.0.7",
    "expo-build-properties": "~1.0.10",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.9",
    "expo-store-review": "~9.0.9",
    "expo-web-browser": "~15.0.9",
    "i18next": "^25.6.3",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-hook-form": "^7.69.0",
    "react-i18next": "^16.3.5",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-picker-select": "^9.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "0.5.1",
    "zod": "^4.2.1",
    "zustand": "^5.0.8"
  },
  "private": true,
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^15.0.3",
    "expo-atlas": "^0.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
